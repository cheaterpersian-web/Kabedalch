# PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/app

# Redis
REDIS_URL=redis://redis:6379

# JWT
JWT_SECRET=change-me

# Data encryption key (base64 of 32 bytes)
